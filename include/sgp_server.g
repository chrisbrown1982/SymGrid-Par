SolMat:=function(p, x)
  local e, r;
  e:=One(GF(p));
  if Determinant(x[1]*e) = 0*Z(p) then
    return([0]);
  fi;
  Print(x);
  r:=SolutionMat(TransposedMat(x[1]*e),x[2]*e);
  Print(r);
  return([List(r, Int), p]);
end;

CRAMat:=function(ps, r)
local x, pris, vecs, res;
pris:=[];
vecs:=[];
for x in r do
  Print(x);
  if not x = [0] then
    pris:=Concatenation(pris,[x[2]]);
    vecs:=Concatenation(vecs,[x[1]]);
  fi;
od;
#Print(pris);
#Print(vecs);
Print(ps);
Print(r);
res:=ChineseRem(pris, vecs);
return(res);
end;

