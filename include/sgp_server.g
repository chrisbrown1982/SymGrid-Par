Fib1:=function(s, x)
#return (((Fibonacci ((x mod 20) + 10)) + x) mod s);
return ((x +1) mod s);
end;

Fib2:=function(s, x)
#return (((Fibonacci ((x mod 10) + 20)) + x) mod s);
return ((x+2) mod s);
end;

Fib3:=function(s, x)
#return (((Fibonacci ((x mod 19) + 10)) + x-1) mod s);
return ((x+3) mod s);
end;

